<Window x:Class="ICSheet5e.Views.HealthChangeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="HealthChangeWindow" Height="100" Width="200">
    <StackPanel>
        <StackPanel Orientation="Horizontal" Margin="0,0,0,8">
            <Label Content="{Binding Path=ChangeTypeDescription}" Width="80"/>
            <TextBox Width="100"></TextBox>
        </StackPanel>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
            <Button Command="{Binding Path=DoHealthChangeCommand}" IsDefault="True" Margin="0,0,8,0">OK</Button>
            <Button IsCancel="True">Cancel</Button>
        </StackPanel>
    </StackPanel>
</Window>
